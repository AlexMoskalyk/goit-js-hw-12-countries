{{#each this}}
<li>
    <h2>{{name}}</h2>
    <h3>Capital: {{capital}}</h3>
    <h3>Population: {{population}}</h3>
    
    <h3>Languages:</h3>
    <ul>
    {{#each languages}}
         <li>{{name}}</li>
    {{/each}}

    </ul>
    <img src="{{flag}}" alt="{{demonym}}" width="250">
</li>
{{/each}}